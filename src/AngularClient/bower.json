{
    "name": "bower",
    "license": "Apache-2.0",
    "private": true,
    "dependencies": {
        "bootstrap": "3.3.2",
		"jquery": "1.10.2",
        "angular": "1.3.15",
        "angular-ui-router": "0.2.13",
        "angular-local-storage": "0.1.5"
    },
	"exportsOverride": {
		"bootstrap": {
			"js": "dist/js/*.*",
			"css": "dist/css/*.*",
			"fonts": "dist/fonts/*.*"
		},
        "jquery": {
            "js": "jquery.{js,min.js,min.map}"
        },
        "angular": {
			"": "angular{.js,.min.js,.min.js.map, .min.js.gzip}",
			"css": "angular-csp.css"
		},
        "angular-ui-router": {
            "": "release/angular-ui-router.{js,min.js,min.js.map}"
        },
        "angular-local-storage": {
            "": "dist/*.*"
        }
	}
}
